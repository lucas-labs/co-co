{
	"name": "@lucas-labs/coco",
	"version": "1.0.0",
	"license": "MIT",
	"description": "ðŸ¥¥ coco â€º an interactive cli for creating conventional commits.",
	"bin": {
		"coco": "bin/coco.js"
	},
	"author": "Lucas Colombo <lucas@lucaslabs.tech> (https://lucaslabs.tech/)",
	"scripts": {
		"coco": "node bin/coco.js",
		"build": "tsc",
		"dev": "ts-node --project tsconfig.json src/coco.tsx",
		"pretest": "npm run build",
		"test": "xo && ava",
		"global": "pnpm rimraf bin & tsc & npm uninstall -g @lucas-labs/coco & npm i -g ."
	},
	"engines": {
		"node": ">=10"
	},
	"files": [
		"bin",
		"bin/coco.js"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/lucas-labs/coco.git"
	},
	"bugs": {
		"url" : "https://github.com/lucas-labs/coco/issues",
		"email" : "lucas@lucaslabs.tech"
	},
	"keywords" : [
		"commit",
		"commits",
		"conventional",
		"cli",
		"conventional commit",
		"conventional commits",
		"node",
		"ink",
		"client",
		"command",
		"line"
	],
	"ava": {
		"typescript": {
			"extensions": [
				"tsx"
			],
			"rewritePaths": {
				"src/": "bin/"
			}
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	},
	"dependencies": {
		"@lucas-labs/copy-paste": "^1.3.2",
		"emoji-regex": "^10.1.0",
		"ink": "^3.2.0",
		"ink-use-stdout-dimensions": "^1.0.5",
		"install": "^0.13.0",
		"just-left-pad": "^3.1.1",
		"just-right-pad": "^3.1.1",
		"lcid": "^3.1.1",
		"meow": "^9.0.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"string-width": "4.2.3",
		"strip-ansi": "6.0.1"
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.21",
		"ava": "^4.3.3",
		"chalk": "^4.1.2",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.31.8",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^2.1.0",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3",
		"xo": "^0.39.1"
	}
}
